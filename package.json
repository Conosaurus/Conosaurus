{
  "name": "conosaurus",
  "description": "The comic convention planning website",
  "keywords": [
    "Conosaurus",
    "comic",
    "convention"
  ],
  "author": "John Wojtkielewicz",
  "bugs": {
    "url": "https://github.com/Conosaurus/Conosaurus/issues"
  },
  "homepage": "https://github.com/Conosaurus/Conosaurus#readme",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Conosaurus/Conosaurus.git"
  },
  "version": "0.0.0",
  "main": "src/app.js",
  "scripts": {
    "build": "webpack --mode production",
    "setup:database": "node --no-warnings ./scripts/setup-database.js",
    "start": "node ./src/app.js",
    "start:watch": "npm-watch start"
  },
  "dependencies": {
    "@azure/cosmos": "^3.6.2",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "npm-watch": "^0.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "watch": {
    "start": {
      "extensions": [
        "js"
      ],
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
